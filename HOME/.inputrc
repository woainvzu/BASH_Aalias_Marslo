set completion-ignore-case on
set show-all-if-ambiguous on
set show-all-if-unmodified on

set keymap vi-insert
set editing-mode vi

# Command Mode
set keymap vi-command
v: ""

# Insert Mode
set keymap vi-insert

"\C-k": kill-line
"\C-u": unix-line-discard

"\C-p": previous-history
"\C-n": next-history
"\C-a": beginning-of-line
"\C-e": end-of-line
"\C-f": forward-char
"\C-b": backward-char

"\et": transpose-words
"\ef": forward-word
"\eb": backward-word
"\ed": kill-word
"\C-w": unix-word-rubout

"\C-d": delete-char
"\C-h": backward-delete-char

"\e.":yank-last-arg
"\e_": yank-last-arg

"\C-_": undo
